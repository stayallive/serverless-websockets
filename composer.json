{
    "name": "stayallive/serverless-websockets",
    "description": "Websockets for Serverless.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Bouma",
            "email": "me@alexbouma.me",
            "homepage": "https://alex.bouma.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "async-aws/core": "^1.7",
        "async-aws/dynamo-db": "^1.0.1",
        "async-aws/sqs": "^1.3",
        "aws/aws-sdk-php": "^3.164",
        "bref/bref": "^1.0",
        "bref/logger": "^1.0",
        "illuminate/support": "^8.15",
        "php-di/slim-bridge": "^3.0",
        "pusher/pusher-php-server": "^4.1",
        "slim/psr7": "^1.2",
        "slim/slim": "^4.6",
        "symfony/http-client": "^5.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Stayallive\\ServerlessWebSockets\\": "src"
        }
    },
    "scripts": {
        "phpcs": "vendor/bin/php-cs-fixer fix",
        "phpcs-ci": "vendor/bin/php-cs-fixer fix --dry-run --diff"
    },
    "config": {
        "sort-packages": true
    }
}
